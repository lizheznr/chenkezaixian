.header{
  font-family: Arial, Helvetica, sans-serif;
  font-weight: 900;
  text-align: center;
  margin-top: 8px;
  margin-bottom: 10px;
}
.table{
  border:1px solid #ccc;
  width:100%;
  border-right:0;
  border-bottom:0;
  margin-left:1%;
}
.tr{
  width:100%;
  display: flex;
  justify-content: space-between;
  border:1px solid #ccc;
}
.th{
  font-weight: 900;
  font-size: 14px;
  background:#1E90FF;
  color:rgb(0, 0, 1);
}
.td{
  padding: 10px;
  width:20px;
  text-align: center;
  background: rgba(144, 200, 247, 0.133);
  border-bottom: 1px solid #ccc;
  border-right: 1px solid #ccc;
}
.th1,.td1{
  width:40%;
  text-align: center;
}
.th2,.th3,.td2,.td3{
  width:25%;
  text-align: center;
}
.th4,.td4{
  width:20%;
  text-align: center;
}